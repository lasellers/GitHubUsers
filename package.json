{
  "name": "githubusers",
  "fullName": "GitHub Users",
  "version": "3.0.9",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ngcc && ng build",
    "rebuild": "npm clean-install && ng build",
    "test": "ng test --watch=false --reporters=spec",
    "ci": "npm run test --code-coverage --no-watch --no-progress --browsers=ChromeHeadless --reporters=spec,kjhtml,coverage-istanbul,junit,htmlDetailed",
    "test-ci": "ng test --code-coverage --watch=false --browsers=ChromeHeadless --reporters=spec,kjhtml,coverage-istanbul,junit,htmlDetailed",
    "coverage": "ng test --code-coverage --progress --watch=false --reporters=spec,kjhtml,coverage-istanbul,junit,htmlDetailed",
    "test-dev": "ng test --code-coverage --progress --watch=true --browsers=Chrome",
    "test-sr": "npm run test --singleRun=true",
    "lint": "eslint --ext ts src/app",
    "lint-fix": "eslint --ext ts --fix src/app",
    "lint-dry": "eslint --ext ts --fix-dry-run src/app",
    "lint-test": "eslint --ext ts src/test/**/*.ts",
    "lint-old": "ng lint",
    "test-e2e": "npm run e2e -- --protractor/protractor.conf.ts",
    "e2e-ng": "ng e2e",
    "e2e": " ./node_modules/protractor/bin/protractor e2e/protractor.conf.ts",
    "test-mocha": "mocha --recursive src/test/*.js",
    "test-nyc": "nyc ng test --code-coverage --progress",
    "test-simple": "ng test",
    "e2e-report": "node e2e/cucumber-html-reporter.js",
    "e2e-prep": "node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager clean && node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager update",
    "e2e-v": "npm ls webdriver-manager",
    "e2e-versions": "npm ls webdriver-manager",
    "e2e-drivers": "node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager status"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/forms": "^9.1.12",
    "@angular/localize": "^9.1.12",
    "@angular/material": "^10.1.0",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@ngrx/store": "^9.2.0",
    "core-js": "^3.6.5",
    "ngx-toastr": "^12.0.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "webpack": "^4.43.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.10",
    "@angular/cli": "^9.1.10",
    "@angular/compiler-cli": "^9.1.12",
    "@ngrx/schematics": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.48",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "angular-cli-ghpages": "^0.6.2",
    "chai": "^4.2.0",
    "chai-datetime": "^1.7.0",
    "chai-http": "^4.3.0",
    "codelyzer": "^5.1.2",
    "cucumber": "^6.0.5",
    "cucumber-html-report": "^0.6.5",
    "cucumber-html-reporter": "^5.2.0",
    "cucumber-pretty": "^6.0.0",
    "eslint": "^7.4.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-html-detailed-reporter": "^2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-summary-reporter": "^1.8.0",
    "karma-typescript": "^5.0.3",
    "mocha": "^8.0.1",
    "mocha-sinon": "^2.1.2",
    "nyc": "^15.1.0",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "puppeteer": "^4.0.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
